{
    "name": "enumeum/doctrine-enums-bundle",
    "description": "Bundle for Doctrine enumerations extension for Postgres",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "enumeum",
        "enum",
        "enumeration",
        "doctrine",
        "extensions",
        "postgres"
    ],
    "authors": [
        {
            "name": "Alexey Sitka",
            "email": "alexey.sitka@gmail.com"
        }
    ],
    "support": {
        "email": "alexey.sitka@gmail.com"
    },
    "require": {
        "php": "^8.1",
        "doctrine/migrations": "^3.2",
        "enumeum/doctrine-enums": "^2.0",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "doctrine/sql-formatter": "^1.0",
        "ergebnis/composer-normalize": "^2.28",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nesbot/carbon": "^2.55",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "conflict": {
        "doctrine/dbal": "<2.13.1 || ^3.0 <3.2",
        "doctrine/orm": "<2.10.2",
        "sebastian/comparator": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "Enumeum\\DoctrineEnumBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnumeumBundleTests\\": "tests/Enumeum/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {},
    "scripts": {
        "fix-cs": "php-cs-fixer fix --config=.php-cs-fixer.dist.php"
    }
}
